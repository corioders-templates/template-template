		"lint": "eslint ."
	},
	"devDependencies": {
		"@corioders/templates-manager-types": "^0.0.2",
		"@trivago/prettier-plugin-sort-imports": "^2.0.4",
		"@types/node": "^16.9.4",
		"@typescript-eslint/eslint-plugin": "^4.28.3",
		"@typescript-eslint/parser": "^4.28.3",
		"eslint": "^7.32.0",
		"nodemon": "^2.0.12",
		"prettier": "^2.4.1",
		"tsc-alias": "^1.3.9",
		"typescript": "^4.4.3"
	}
}
