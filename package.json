{
	"name": "template-template",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"clean": "rm -rf out",
		"watch": "tsc --watch & tsc-alias --watch --silent & nodemon --quiet --ext ts --watch src --exec 'eslint .'",
